{
  "name": "ogini",
  "version": "0.0.1",
  "description": "Ogini - A powerful search engine with Nigerian roots",
  "author": "Precious Atam",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run copy-docs",
    "copy-docs": "mkdir -p dist/api/documentation/docs && cp -r src/api/documentation/docs/* dist/api/documentation/docs/ || true",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:int": "jest --config ./test/jest-int.json",
    "test:performance": "jest --testNamePattern=performance",
    "test:memory": "cd scripts/testing && bash run-memory-tests.sh",
    "docker:dev": "sudo docker-compose up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:run": "docker run -p 3000:3000 ogini",
    "heap:profile": "node --expose-gc --inspect=0.0.0.0:9229 profile-indexing.js",
    "performance:test": "bash scripts/run-performance-tests.sh",
    "performance:report": "node scripts/generate-performance-report.js",
    "cleanup:heap": "./scripts/cleanup-heap-snapshots.sh",
    "coverage:verify": "./scripts/verify-coverage.sh",
    "demo:postgresql": "npx ts-node scripts/demo-postgresql-search.ts",
    "demo:api": "./scripts/demo-api-endpoints.sh",
    "setup:postgresql": "./scripts/setup-postgresql-demo.sh",
    "setup:postgresql:init": "psql -h $POSTGRES_HOST -p $POSTGRES_PORT -U $POSTGRES_USER -d $POSTGRES_DB -f scripts/init-postgres.sql",
    "setup:postgresql:dev": "psql -h localhost -p 5432 -U postgres -d ogini_search_dev -f scripts/init-postgres.sql",
    "setup:postgresql:prod": "psql -h $POSTGRES_HOST -p $POSTGRES_PORT -U $POSTGRES_USER -d $POSTGRES_DB -f scripts/init-postgres.sql",
    "test:postgresql": "npx ts-node scripts/test-postgresql-connection.ts",
    "ts-node": "npx ts-node",
    "verify-counts": "npx ts-node scripts/verify-document-counts.ts"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^11.0.13",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.13",
    "@nestjs/platform-express": "^11.0.13",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.1.6",
    "@nestjs/typeorm": "^11.0.0",
    "body-parser": "^2.2.0",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "classic-level": "^3.0.0",
    "encoding-down": "^7.1.0",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "levelup": "^5.1.1",
    "marked": "^15.0.11",
    "porter-stemmer": "^0.9.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^11.0.13",
    "@types/body-parser": "^1.19.6",
    "@types/bull": "^4.10.0",
    "@types/encoding-down": "^5.0.5",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/levelup": "^5.1.5",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.10.0",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "pg": "^8.16.3",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".*\\.(spec|int-spec|e2e-spec)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^packages/(.*)$": "<rootDir>/packages/$1"
    }
  }
}
