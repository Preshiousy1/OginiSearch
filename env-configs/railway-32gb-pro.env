# ===================================================
# PHASE 1: RAILWAY 32GB RAM / 32 vCPU PRO OPTIMIZATION
# ===================================================

# === CORE QUEUE CONCURRENCY (CRITICAL) ===
INDEXING_CONCURRENCY=80
BULK_INDEXING_CONCURRENCY=48
DOC_PROCESSING_CONCURRENCY=64

# === BATCH PROCESSING (CRITICAL) ===
BULK_BATCH_SIZE=1000
BULK_MAX_BATCH_SIZE=2500

# === MEMORY OPTIMIZATION (CRITICAL) ===
NODE_OPTIONS=--max-old-space-size=8192 --expose-gc
BULK_MAX_MEMORY_USAGE=0.8
MAX_CACHE_SIZE=25000

# === PERFORMANCE BOOSTERS (CRITICAL) ===
ENABLE_FAST_DEDUPLICATION=true
ENABLE_PARALLEL_ANALYSIS=true
ANALYSIS_WORKERS=16

# === RAILWAY PRO OPTIMIZATIONS (CRITICAL) ===
RAILWAY_MEMORY_LIMIT=32768
RAILWAY_CPU_LIMIT=32
RAILWAY_MAX_CONNECTIONS=25 